<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4CAF50"
    android:backgroundTint="#FCF5F8F7"
    android:hapticFeedbackEnabled="false"
    tools:context=".Dataupload">


    <ImageView
        android:id="@+id/img1"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:layout_marginTop="28dp"

        android:src="@drawable/placeholder"
        app:layout_constraintEnd_toStartOf="@+id/img2"
        app:layout_constraintHorizontal_bias="0.744"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img2"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:layout_marginTop="28dp"

        android:layout_marginEnd="4dp"
        android:src="@drawable/placeholder"
        app:layout_constraintEnd_toStartOf="@+id/img3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img3"
        android:layout_width="120dp"
        android:layout_height="150dp"
        android:layout_marginTop="28dp"

        android:layout_marginEnd="4dp"

        android:src="@drawable/placeholder"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:hint="Phone"
        android:textColorHint="@color/black"
        android:inputType="phone"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/adress"
        app:layout_constraintEnd_toStartOf="@+id/rent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img3"
        app:layout_constraintVertical_bias="0.778" />

    <EditText
        android:id="@+id/rent"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="Rent"
        android:inputType="number"
        android:textColor="#1B1919"
        android:textColorHint="#171515"
        android:textColorLink="#C6C3C3"
        app:layout_constraintBottom_toTopOf="@+id/talent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img3"
        app:layout_constraintVertical_bias="0.671" />

    <MultiAutoCompleteTextView
        android:id="@+id/desi"
        android:layout_width="250dp"
        android:layout_height="80dp"
        android:layout_marginBottom="36dp"
        android:hint="for Grils,boys,things and your point"
        android:textColor="#151414"
        android:textColorHint="#131313"
        android:textColorLink="#DACDCD"
        app:layout_constraintBottom_toTopOf="@+id/upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/talent"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="320dp"
        android:ems="10"
        android:hint="tanent  counting"
        android:inputType="number"
        android:textColor="#201E1E"
        android:textColorHint="#0E0D0D"
        android:textColorLink="#B8B0B0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/adress"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="320dp"
        android:ems="10"
        android:hint="Adress"
        android:inputType="textPostalAddress"
        android:textColor="#171717"
        android:textColorHint="#121111"
        android:textColorLink="#BFBBBB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/talent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="72dp"
        android:text="Click "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>